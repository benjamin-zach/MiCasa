<Window x:Class="MiCasaUI.ChooseCategoryWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MiCasaUI"
        mc:Ignorable="d"
        Title="Choose Category for new Ingredient " Height="70" Width="250">
    <Grid>
        <ComboBox Margin="5 5 5 5" Name="categoryComboBox" HorizontalAlignment="Stretch"/>
        <Button Margin="5 5 5 5" Name="okButton" Content="OK" HorizontalAlignment="Right" Width="40"/>
    </Grid>
</Window>
